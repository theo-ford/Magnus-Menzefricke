{
  "format": "custom",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "project_type": {
        "type": "Select",
        "config": {
          "label": "Project Type",
          "placeholder": "",
          "options": ["Design Management", "Architecture", "Masterplanning"],
          "default_value": "Design Management"
        }
      },
      "design_architects": {
        "type": "Text",
        "config": {
          "label": "Design Architects",
          "placeholder": ""
        }
      },
      "technical_architects": {
        "type": "Text",
        "config": {
          "label": "Technical Architects",
          "placeholder": ""
        }
      },
      "net_internal_area": {
        "type": "Text",
        "config": {
          "label": "Net Internal Area",
          "placeholder": ""
        }
      },
      "completion_date": {
        "type": "Text",
        "config": {
          "label": "Completion Date",
          "placeholder": ""
        }
      },
      "client": {
        "type": "Text",
        "config": {
          "label": "Client",
          "placeholder": ""
        }
      },
      "role": {
        "type": "Text",
        "config": {
          "label": "Role",
          "placeholder": ""
        }
      },
      "location": {
        "type": "Text",
        "config": {
          "label": "Location",
          "placeholder": ""
        }
      },
      "description": {
        "type": "Text",
        "config": {
          "label": "Description",
          "placeholder": ""
        }
      },
      "construction_value": {
        "type": "Text",
        "config": {
          "label": "Construction Value",
          "placeholder": ""
        }
      },
      "related_project_image": {
        "type": "Image",
        "config": {
          "label": "Related Project Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "related_projects": {
        "type": "Boolean",
        "config": {
          "label": "Related Projects",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": true
        }
      },
      "related_project_1": {
        "type": "Link",
        "config": {
          "label": "Related Project 1",
          "select": "document",
          "repeat": false,
          "customtypes": ["project"]
        }
      },
      "related_project_2": {
        "type": "Link",
        "config": {
          "label": "Related Project 2",
          "select": "document",
          "repeat": false
        }
      }
    },
    "Homepage Content": {
      "slices1": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": ""
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": ""
        }
      }
    },
    "Project Page Content": {
      "hero_image_desktop": {
        "type": "Image",
        "config": {
          "label": "Hero Image Desktop",
          "constraint": {},
          "thumbnails": []
        }
      },
      "hero_image_mobile_ipad": {
        "type": "Image",
        "config": {
          "label": "Hero Image Mobile & iPad",
          "constraint": {},
          "thumbnails": []
        }
      },
      "hero_text_color": {
        "type": "Select",
        "config": {
          "label": "Hero Text Color",
          "placeholder": "",
          "options": ["White", "Black"],
          "default_value": "White"
        }
      },
      "slices3": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "rich_text": {
              "type": "SharedSlice"
            },
            "project_page_image": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
